COLUMN TABLE "TERUMODRMS_DB.db.Tables::TERUMO_HOSPITAL_MASTER"(
	CODE BIGINT NOT NULL,
	DESCRIPTION NVARCHAR(100),
	PHONE NVARCHAR(13),
	EMAIL NVARCHAR(50),
	ADDRESS1 NVARCHAR(150),
	ADDRESS2 NVARCHAR(150),
	CITY NVARCHAR(50),
	PINCODE INTEGER,
	LAST_UPDATED TIMESTAMP,
	CREATED_ON TIMESTAMP,
	BU_SUB_CODE NVARCHAR(100),
	DISTRIBUTOR_ID NVARCHAR(100),
	START_DATE DATE,
	END_DATE DATE,
	HOSP_TYPE NVARCHAR(100),
	STATE NVARCHAR(50),
	HOSP_GROUP NVARCHAR(50),
	PRIMARY KEY(CODE)
)
