VIEW "TERUMODRMS_DB.db.View::TERUMO_GRN_COUNT" AS
SELECT  
     
          A."CUSTOMER_CODE" as DISTRIBUTOR_ID,
          COUNT(A."MATERIAL") as PENDING_GRN
          
FROM "TERUMODRMS_DB.db.Tables::TERUMO_INVOICE_MASTER" as A
WHERE NOT EXISTS(SELECT NULL FROM "TERUMODRMS_DB.db.Tables::TERUMO_GRN" as B WHERE B."INVOICE_NO" =  A."INVOICE_NO")
GROUP BY  A."CUSTOMER_CODE"