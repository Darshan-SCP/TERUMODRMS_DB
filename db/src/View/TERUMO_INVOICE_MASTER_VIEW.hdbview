VIEW "TERUMODRMS_DB.db.View::TERUMO_INVOICE_MASTER_VIEW" AS
SELECT  
     
          TO_BIGINT(A."SO_NUMBER") as SO_NUMBER,
          A."INVOICE_NO"  as "INVOICE_NO",
          A."INVOICE_DATE"  as "INVOICE_DATE",
          A."DEL_DATE"  as "DEL_DATE"
          
FROM "TERUMODRMS_DB.db.Tables::TERUMO_INVOICE_MASTER" as A
GROUP BY  A."SO_NUMBER",A."INVOICE_NO",A."INVOICE_DATE" ,A."DEL_DATE"