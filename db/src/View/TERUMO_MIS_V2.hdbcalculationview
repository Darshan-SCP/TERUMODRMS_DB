    <viewAttributes>
      <viewAttribute id="YEAR"/>
      <viewAttribute id="BU_CODE"/>
      <viewAttribute id="SUB_DIV"/>
      <viewAttribute id="APRIL"/>
      <viewAttribute id="MAY"/>
