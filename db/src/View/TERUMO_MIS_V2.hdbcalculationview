  <calculationView xsi:type="Calculation:JoinView" id="FINAL_JOIN" cardinality="CN_N" joinType="leftOuter">
    <viewAttributes>
      <viewAttribute id="BU_CODE"/>
      <viewAttribute id="FINANCIAL_YEAR"/>
      <viewAttribute id="PLAN" aggregationType="sum"/>
      <viewAttribute id="FORECAST" aggregationType="sum"/>
      <viewAttribute id="ORDER_IN_HAND" aggregationType="sum"/>
      <viewAttribute id="PI_RAISED" aggregationType="sum"/>
      <viewAttribute id="NON_STOCK" aggregationType="sum"/>
      <viewAttribute id="CN_PROVISION" aggregationType="sum"/>
      <viewAttribute id="SALE_IN_TRANSIT" aggregationType="sum"/>
    </viewAttributes>
    <calculatedViewAttributes>
      <calculatedViewAttribute id="AVAILABLE_FOR_BILLING" datatype="DOUBLE" expressionLanguage="SQL">
        <formula>CASE 
WHEN (&quot;PI_RAISED&quot;) IS NOT NULL AND (&quot;NON_STOCK&quot;) IS NOT NULL
THEN (&quot;PI_RAISED&quot; - &quot;NON_STOCK&quot;)
WHEN (&quot;PI_RAISED&quot;) IS NULL AND (&quot;NON_STOCK&quot;) IS NOT NULL
THEN ( 0 - &quot;NON_STOCK&quot; )
WHEN (&quot;PI_RAISED&quot;) IS NOT NULL AND (&quot;NON_STOCK&quot;) IS NULL
THEN ( &quot;PI_RAISED&quot; - 0 )
ELSE 0
END</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="MTD_GROSS_SALE" datatype="DOUBLE" expressionLanguage="SQL">
        <formula>CASE 
WHEN (&quot;AVAILABLE_FOR_BILLING&quot;) IS NOT NULL AND (&quot;SALE_IN_TRANSIT&quot;) IS NOT NULL
THEN (&quot;AVAILABLE_FOR_BILLING&quot; - &quot;SALE_IN_TRANSIT&quot;)
WHEN (&quot;AVAILABLE_FOR_BILLING&quot;) IS NULL AND (&quot;SALE_IN_TRANSIT&quot;) IS NOT NULL
THEN ( 0 - &quot;SALE_IN_TRANSIT&quot; )
WHEN (&quot;AVAILABLE_FOR_BILLING&quot;) IS NOT NULL AND (&quot;SALE_IN_TRANSIT&quot;) IS NULL
THEN ( &quot;AVAILABLE_FOR_BILLING&quot; - 0 )
ELSE 0
END</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="NET_SALES" datatype="DOUBLE" expressionLanguage="SQL">
        <formula>CASE 
WHEN (&quot;MTD_GROSS_SALE&quot;) IS NOT NULL AND (&quot;CN_PROVISION&quot;) IS NOT NULL
THEN (&quot;MTD_GROSS_SALE&quot; - &quot;CN_PROVISION&quot;)
WHEN (&quot;MTD_GROSS_SALE&quot;) IS NULL AND (&quot;CN_PROVISION&quot;) IS NOT NULL
THEN ( 0 - &quot;CN_PROVISION&quot; )
WHEN (&quot;MTD_GROSS_SALE&quot;) IS NOT NULL AND (&quot;CN_PROVISION&quot;) IS NULL
THEN ( &quot;MTD_GROSS_SALE&quot; - 0 )
ELSE 0
END</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="PERCENT_TO_PLAN" datatype="DOUBLE" expressionLanguage="SQL">
        <formula>CASE 
WHEN (&quot;MTD_GROSS_SALE&quot;) IS NOT NULL AND (&quot;MTD_GROSS_SALE&quot;) &gt; 0  AND (&quot;PLAN&quot;) IS NOT NULL AND (&quot;PLAN&quot;) &gt; 0
THEN ((&quot;MTD_GROSS_SALE&quot; / &quot;PLAN&quot;) * 100)
ELSE 0
END</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="PERCENT_TO_FC" datatype="DOUBLE" expressionLanguage="SQL">
        <formula>CASE 
WHEN (&quot;NET_SALES&quot;) IS NOT NULL AND (&quot;NET_SALES&quot;) &gt;  0 AND (&quot;FORECAST&quot;) IS NOT NULL AND (&quot;FORECAST&quot;) &gt; 0
THEN ((&quot;NET_SALES&quot; / &quot;FORECAST&quot;) * 100)
ELSE 0
END</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="PI_OPEN_FOR_PAYMENT" datatype="DOUBLE" expressionLanguage="SQL">
        <formula>CASE 
WHEN (&quot;PI_RAISED&quot;) IS NOT NULL AND (&quot;AVAILABLE_FOR_BILLING&quot;) IS NOT NULL
THEN (&quot;PI_RAISED&quot; - &quot;AVAILABLE_FOR_BILLING&quot;)
WHEN (&quot;PI_RAISED&quot;) IS NULL AND (&quot;AVAILABLE_FOR_BILLING&quot;) IS NOT NULL
THEN ( 0 - &quot;AVAILABLE_FOR_BILLING&quot; )
WHEN (&quot;PI_RAISED&quot;) IS NOT NULL AND (&quot;AVAILABLE_FOR_BILLING&quot;) IS NULL
THEN ( &quot;PI_RAISED&quot; - 0 )
ELSE 0
END</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="GAP_TO_FC" datatype="DOUBLE" expressionLanguage="SQL">
        <formula>CASE 
WHEN (&quot;NET_SALES&quot;) &gt; (&quot;FORECAST&quot;)
THEN 0
ELSE (&quot;FORECAST&quot; - (&quot;NET_SALES&quot; + &quot;PI_OPEN_FOR_PAYMENT&quot;))
END</formula>
      </calculatedViewAttribute>
    </calculatedViewAttributes>
    <input node="FORECAST_PL_OIH">
      <mapping xsi:type="Calculation:AttributeMapping" target="BU_CODE" source="BU_CODE"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="FINANCIAL_YEAR" source="FINANCIAL_YEAR"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="ORDER_IN_HAND" source="ORDER_IN_HAND"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="PLAN" source="PLAN"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="FORECAST" source="FORECAST"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="CN_PROVISION" source="CN_PROVISION"/>
    </input>
    <input node="SIT_PIR_NONSTCK">
      <mapping xsi:type="Calculation:AttributeMapping" target="PI_RAISED" source="PI_RAISED"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="NON_STOCK" source="NON_STOCK"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="SALE_IN_TRANSIT" source="SALE_IN_TRANSIT"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="BU_CODE" source="BUCODE"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="FINANCIAL_YEAR" source="FINANCIAL_YEAR"/>
    </input>
    <joinAttribute name="BU_CODE"/>
    <joinAttribute name="FINANCIAL_YEAR"/>
  </calculationView>
  <calculationView xsi:type="Calculation:AggregationView" id="SIT_AGG">
    <viewAttributes>
      <viewAttribute id="BUCODE"/>
      <viewAttribute id="SALE_IN_TRANSIT" aggregationType="sum"/>
      <viewAttribute id="FINANCIAL_YEAR"/>
      <viewAttribute id="MONTH"/>
