TYPE "TERUMODRMS_DB.db.Structure::ST_TERUMO_RGA_HEADER" AS TABLE(
	RGA_NO BIGINT NOT NULL,
	DISTRIBUTOR_ID NVARCHAR(100),
	DISTRIBUTOR_REASON NVARCHAR(100),
	STATUS INTEGER,
	APPROVER_LEVEL INTEGER,
	APPROVER_ROLE NVARCHAR(50),
	NEXT_APPROVER NVARCHAR(100),
	BU_CODE NVARCHAR(50),
	BU_SUB_CODE NVARCHAR(100),
	SAP_RETURN_ORDER NVARCHAR(10),
	CREATED_ON TIMESTAMP,
	PRIMARY KEY(RGA_NO)
)