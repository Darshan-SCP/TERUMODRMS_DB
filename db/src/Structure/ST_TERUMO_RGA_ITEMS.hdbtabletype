TYPE "TERUMODRMS_DB.db.Structure::ST_TERUMO_RGA_ITEMS" AS TABLE(
	RGA_NO BIGINT NOT NULL,
	RGA_ITEM_NO INTEGER NOT NULL,
	ITEM_CODE NVARCHAR(30),
	BATCH NVARCHAR(20),
	EXPIRY_DATE DATE,
	SALEABLE VARCHAR(1),
	INVOICE_NO BIGINT,
	INVOICE_DATE DATE,
	INVOICE_QUANTITY INTEGER,
	PRICE DOUBLE,
	EXTENDED DOUBLE,
	RETURN_QUANTITY INTEGER,
	ACCEPTED_QUANTITY INTEGER,
	ACCEPTED_PRICE DOUBLE,
	PRIMARY KEY(RGA_NO, RGA_ITEM_NO)
)