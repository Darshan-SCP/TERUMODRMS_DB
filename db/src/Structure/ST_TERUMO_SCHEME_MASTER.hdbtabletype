TYPE "TERUMODRMS_DB.db.Structure::ST_TERUMO_SCHEME_MASTER" AS TABLE(
	SCH_NO BIGINT NOT NULL,
	DISTRIBUTOR_ID NVARCHAR(40) NOT NULL,
	SCHEME_NAME NVARCHAR(100),
	PROD_GRP NVARCHAR(50),
	PROD_GRP_DESC NVARCHAR(100),
	REQ_QUANTITY INTEGER,
	FOC_QUANTITY INTEGER,
	SCHEME_STATUS NVARCHAR(10), 
	CREATION_DATE TIMESTAMP,
	LAST_UPDATED_DATE TIMESTAMP,
	VALID_FROM DATE,
	VALID_TO DATE,
	PRIMARY KEY(SCH_NO,DISTRIBUTOR_ID)
)
