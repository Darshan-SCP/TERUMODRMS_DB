TYPE "TERUMODRMS_DB.db.Structure::ST_TERUMO_FORM_MASTER" AS TABLE (
	FORM_ID INTEGER NOT NULL,
	PROD_GRP_CODE NVARCHAR(50) NOT NULL,
	PROD_GRP_DESC NVARCHAR(100),
	FACTORY_NAME NVARCHAR(100) NOT NULL,
	FORM_NAME NVARCHAR(100),
	ATTACH_CODE INTEGER,
	FILE_NAME NVARCHAR(100),
	FILE_TYPE NVARCHAR(100),
	FILE_MIMETYPE NVARCHAR(100),
	FILE_CONTENT BLOB,
	UPLOAD_DATE TIMESTAMP,
	LAST_UPDATED TIMESTAMP,
	PRIMARY KEY(FORM_ID, PROD_GRP_CODE, FACTORY_NAME)
)