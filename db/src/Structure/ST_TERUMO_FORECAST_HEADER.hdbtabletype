TYPE "TERUMODRMS_DB.db.Structure::ST_TERUMO_FORECAST_HEADER" AS TABLE(
	FC_NO BIGINT NOT NULL,
	SUB_DIV NVARCHAR(40),
	YEAR NVARCHAR(10),
	SALES_ASSOCIATES NVARCHAR(100),
	FORECAST_MONTH INTEGER,
	FORECAST_STATUS INTEGER,
	CREATION_DATE TIMESTAMP,
	LAST_UPDATED_DATE TIMESTAMP,
	APRROVER_LEVEL INTEGER,
	APRROVER_ROLE NVARCHAR(50),
	NEXT_APPROVER NVARCHAR(100),
	PRIMARY KEY(FC_NO,SALES_ASSOCIATES,SUB_DIV)
)