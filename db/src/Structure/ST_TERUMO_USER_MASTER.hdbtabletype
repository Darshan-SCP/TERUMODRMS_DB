TYPE "TERUMODRMS_DB.db.Structure::ST_TERUMO_USER_MASTER" AS TABLE(
	USER_ID NVARCHAR(100) NOT NULL,
	BU_CODE NVARCHAR(50),
	NAME NVARCHAR(50),
	USER_TYPE NVARCHAR(50),
	USER_ROLE NVARCHAR(50),
	EMAIL NVARCHAR(50),
	PHONE_NO NVARCHAR(10),
	REGION VARCHAR(1),
	ADDRESS VARCHAR(200),
	LAST_UPDATED TIMESTAMP,
	CREATED_ON TIMESTAMP,
	PRIMARY KEY(USER_ID ,USER_ROLE , EMAIL)
)