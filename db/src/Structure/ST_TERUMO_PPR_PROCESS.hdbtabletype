TYPE "TERUMODRMS_DB.db.Structure::ST_TERUMO_PPR_PROCESS" AS TABLE(
	PPR_NO BIGINT NOT NULL,
	PROD_GRP NVARCHAR(50) NOT NULL,
	PROD_CODE NVARCHAR(50),
	PROD_UNKNOWN VARCHAR(1),	
	FACTORY_NAME NVARCHAR(100) NOT NULL,
	DESCRIPTION NVARCHAR(100),
	PROD_UNKNOWN_DESC NVARCHAR(100),
	STATUS INTEGER,
	SALES_ASSOCIATE_ID NVARCHAR(100),
	APRROVER_ROLE NVARCHAR(50),
	NEXT_APPROVER NVARCHAR(100),
	CREATED_ON TIMESTAMP,
	PRIMARY KEY(PPR_NO, PROD_GRP, FACTORY_NAME)
)