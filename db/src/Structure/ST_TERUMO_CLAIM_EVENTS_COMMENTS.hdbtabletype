TYPE "TERUMODRMS_DB.db.Structure::ST_TERUMO_CLAIM_EVENTS_COMMENTS" AS TABLE(
	CR_NO BIGINT NOT NULL,
	EVENT_NO INTEGER NOT NULL,	
	EVENT_CODE INTEGER,
	USER_ID NVARCHAR(100),
	USER_NAME NVARCHAR(50),
	USER_ROLE NVARCHAR(50),
	REMARK NVARCHAR(100),
	COMMENT	NVARCHAR(100),
	CREATION_DATE TIMESTAMP,
	PRIMARY KEY(CR_NO, EVENT_NO)
)